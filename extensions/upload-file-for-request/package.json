{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "upload-file-for-request",
  "title": "Upload File For Request",
  "description": "upload file for api",
  "icon": "command-icon.png",
  "author": "jinx-1120",
  "categories": [
    "Applications",
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "upload-selected-image-in-finder",
      "title": "Upload Selected Image in Finder",
      "description": "Upload the selected image in Finder to ImageKit",
      "mode": "view"
    },
    {
      "name": "upload-image-in-clipboard",
      "title": "Upload Image in the Clipboard",
      "description": "Upload the image in clipboard",
      "mode": "view"
    },
    {
      "name": "history",
      "title": "Uploaded Images History",
      "description": "List recently uploaded images",
      "mode": "view"
    },
    {
      "name": "index",
      "title": "Upload File For Request",
      "description": "upload file for us3 use api",
      "mode": "view"
    },
    {
      "name": "clear",
      "title": "Clear Upload History",
      "description": "Clear upload history",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.69.3",
    "axios": "^1.6.7",
    "date-fns": "^3.5.0",
    "form-data": "^4.0.0",
    "image-meta": "^0.2.0",
    "run-applescript": "^7.0.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "prettier-eslint": "^16.3.0",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
